        <activity
            android:name="org.redlang.eval.MainActivity"
            android:label="Red on Android" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
